<h1>Sign Up</h1>
<%=render partial: '/errors', locals: {mistake: @user}%>
<%= form_for @user, url:login_path do |f| %>
  <div>
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <%= f.submit "Signup" %>
<% end %>

