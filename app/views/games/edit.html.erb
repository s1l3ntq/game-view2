<h1>Edit Post</h1>

<%= form_for(@game) do |f| %>
<%= f.label(:name) %>
<%= f.text_field :name %>
<%= f.label(:demo) %>
<%= f.text_field :demo %>
<%= f.label(:description) %>
<%= f.text_field :description %>
<%= f.label(:platform) %>
<%= f.text_field :platform %>
<%= f.submit %>
<% end %>